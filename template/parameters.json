{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "germanywestcentral"
        },
        "networkInterfaceName": {
            "value": "template-interface"
        },
        "networkSecurityGroupName": {
            "value": "template-nsg"
        },
        "networkSecurityGroupRules": {
            "value": []
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "template-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.0.0/27"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "template-vm-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "pipDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineName": {
            "value": "template-vm"
        },
        "virtualMachineComputerName": {
            "value": "template-vm"
        },
        "virtualMachineRG": {
            "value": "template"
        },
        "osDiskType": {
            "value": "StandardSSD_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_B2s"
        },
        "nicDeleteOption": {
            "value": "Delete"
        },
        "adminUsername": {
            "value": "azure"
        },
        "adminPassword": {
            "value": null
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        },
        "autoShutdownStatus": {
            "value": "Enabled"
        },
        "autoShutdownTime": {
            "value": "19:00"
        },
        "autoShutdownTimeZone": {
            "value": "UTC"
        },
        "autoShutdownNotificationStatus": {
            "value": "Enabled"
        },
        "autoShutdownNotificationLocale": {
            "value": "en"
        },
        "autoShutdownNotificationEmail": {
            "value": "vidmantas.klastaitis@cloudeon.com"
        }
    }
}